<mapper namespace="com.ali.user_provider.dao.UserDao">
	<select id="queryUserByName" resultType="User">
		select *
		from user
		where name like '%' #{name} '%'
	</select>
	
	<update id="">
		update tbl_user set loginPwd=#{loginPwd} where loginAct=#{loginAct}
	</update>
</mapper>